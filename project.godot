config_version=5

[application]

config/name="Tiny MMO"
config/version="0.0.8"
run/main_scene="res://source/common/main.tscn"
config/features=PackedStringArray("4.5", "GL Compatibility")
config/icon="res://assets/project_icon/icon_svg.svg"

[autoload]

ClientState="*res://source/client/autoload/client_state.gd"

[debug]

